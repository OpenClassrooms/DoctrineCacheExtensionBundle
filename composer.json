{
  "name": "openclassrooms/doctrine-cache-extension-bundle",
  "description": "Symfony2 Bundle extending doctrine cache capabilities ",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Romain Kuzniak",
      "email": "romain.kuzniak@openclassrooms.com"
    },
    {
      "name": "OpenClassrooms",
      "email": "tech@openclassrooms.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "OpenClassrooms\\Bundle\\DoctrineCacheExtensionBundle\\": ""
    }
  },
  "require": {
    "php": ">=7.1",
    "doctrine/doctrine-cache-bundle": "~1.3",
    "openclassrooms/doctrine-cache-extension": "1.0.*@dev",
    "symfony/config": "~3.4 || ~4.0",
    "symfony/dependency-injection": "~3.4 || ~4.0",
    "symfony/http-kernel": "~3.4 || ~4.0",
    "symfony/stopwatch": "~3.4 || ~4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7",
    "satooshi/php-coveralls": "2.1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
