{
  "name": "openclassrooms/doctrine-cache-extension-bundle",
  "description": "Symfony2 Bundle extending doctrine cache capabilities ",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Romain Kuzniak",
      "email": "romain.kuzniak@openclassrooms.com"
    },
    {
      "name": "OpenClassrooms",
      "email": "tech@openclassrooms.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "OpenClassrooms\\Bundle\\DoctrineCacheExtensionBundle\\": ""
    }
  },
  "require": {
    "php": ">=5.6",
    "doctrine/doctrine-cache-bundle": "~1.0",
    "openclassrooms/doctrine-cache-extension": "1.0.*@dev",
    "symfony/config": ">=2.3",
    "symfony/dependency-injection": ">=2.3",
    "symfony/http-kernel": ">=2.3",
    "symfony/stopwatch": ">=2.3"
  },
  "require-dev": {
    "mockery/mockery": "dev-master",
    "phpunit/phpunit": "~5.0",
    "satooshi/php-coveralls": "dev-master"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
